{
    "name": "pixelrpg",
    "version": "0.0.1",
    "description": "Experimental tile based map editor",
    "main": "index.js",
    "type": "module",
    "private": true,
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "yarn workspaces foreach -v -W run build",
        "start": "yarn workspace @pixelrpg/map-editor run start",
        "clear": "yarn workspaces foreach -v --parallel -W run clear",
        "format": "yarn prettier --write .",
        "prettier": "node .yarn/sdks/prettier/index.cjs"
    },
    "author": "Pascal Garber <pascal@artandcode.studio>",
    "license": "MIT",
    "packageManager": "yarn@4.2.1",
    "workspaces": [
        "packages/*"
    ],
    "devDependencies": {
        "prettier": "^3.2.5",
        "typescript": "^5.4.5"
    }
}